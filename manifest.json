{
  "name": "Habit Tracker",

  "version": "1.2",

  "description": "Add personal habits based on your goals and keep track of them.",

  "manifest_version": 3,

  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "permissions": [
     "storage"
   ],

   "icons": {
     "16": "assets/icon16.png",
      "48": "assets/icon48.png",
    "128": "assets/icon128.png" }
}